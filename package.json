{
  "name": "gymjs",
  "version": "0.1.0",
  "description": "An API standard for single-agent reinforcement learning environments",
  "scripts": {
    "dev": "tsdown --watch",
    "build": "tsdown",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./spaces": {
      "types": "./dist/spaces/index.d.ts",
      "default": "./dist/spaces/index.js"
    },
    "./envs/classic_control": {
      "types": "./dist/envs/classic_control/index.d.ts",
      "default": "./dist/envs/classic_control/index.js"
    }
  },
  "files": [
    "dist/**"
  ],
  "repository": "thundraa/gymjs",
  "keywords": [
    "rl",
    "gym",
    "reinforcement learning"
  ],
  "license": "MIT",
  "packageManager": "pnpm@10.7.0",
  "devDependencies": {
    "@types/node": "^24.3.3",
    "prettier": "^3.6.2",
    "tsdown": "^0.15.6",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@tensorflow/tfjs": "^4.22.0",
    "phaser": "^3.90.0"
  }
}
